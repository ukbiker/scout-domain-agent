{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "scout_avatar_icon.png",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/scout_avatar_icon.png",
      "dest": "/scout_avatar_icon.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
